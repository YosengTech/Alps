

@model IEnumerable<Alps.Web.Models.StockInfoViewModel>
@{
	Layout = null;
}
<div class="page page-form-validation clearfix">


	<ol class="breadcrumb breadcrumb-small">
		<li>Forms</li>
		<li class="active"><a href="#/StockInfoViewModel">StockInfoViewModel</a></li>
	</ol>
	<div class="page-wrap">
		<div class="row">
			<table class="table">
				<tr>
					<th>
						@Html.DisplayNameFor(model => model.MaterialName)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.PositionName)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Count)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Quantity)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Owner)
					</th>
				</tr>
				<tr ng-repeat="item in items">
					<td>
						{{item.MaterialName}}
					</td>
					<td>
						{{item.PositionName}}
					</td>
					<td>
						{{item.Count}}
					</td>
					<td>
						{{item.Quantity}}
					</td>
					<td>
						{{item.Owner}}
					</td>
				</tr>
			</table>
		</div>
    </div>
</div>
